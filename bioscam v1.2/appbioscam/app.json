{
  "expo": {
    "name": "Bioscam",
    "slug": "Bioscam",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-google-sign-in",
        {
          "permisssions": [
            "profile",
            "email"
          ]
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bioscamapp.appbioscam"
    },
    "android": {
      "permissions": [
        "CAMERA"
      ],
      "package": "com.bioscamapp.appbioscam",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#C9E8E0"
      }
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "scheme": "appbioscam",
    "extra": {
      "eas": {
        "projectId": "a0b323c4-7044-44bb-b668-701cbec5ad6d"
      }
    }
  }
}
