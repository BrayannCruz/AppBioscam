{
  "expo": {
    "name": "appbioscam",
    "slug": "appbioscam",
    "platforms": 
      ["android",
      "ios"],
    "version": "1.2.20",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A9E11"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
      },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-google-sign-in",
        {
          "permisssions": [
            "profile",
            "email"
          ]
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bioscamapp.appbioscam"
    },
    "android": {
      "permissions": [
        "CAMERA"
      ],
      "package": "com.bioscamapp.appbioscam",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#C9E8E0"
      }
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "scheme": "appbioscam",
    "extra": {
      "eas": {
        //"projectId": "a0b323c4-7044-44bb-b668-701cbec5ad6d",
        "projectId": "appbioscam"
      }
    }
  }
}
